			<section class="parallax parallax-4" style="  background-image:url('<%= image_path('Rocket_Elevator_Assets/Residential_page/elavator1.jpg')  %>')">
				<div class="overlay dark-4"><!-- dark overlay [1 to 9 opacity] --></div>
			</section>
